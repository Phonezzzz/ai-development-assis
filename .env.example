# Конфигурация векторного поиска и умного контекста

# OpenAI API для создания эмбеддингов
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Qdrant конфигурация (векторная база данных)
VITE_QDRANT_URL=http://localhost:6333
VITE_QDRANT_API_KEY=your_qdrant_api_key_here

# Supabase конфигурация (альтернативная векторная БД)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# OpenRouter API для работы с моделями
VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here

# ElevenLabs API для TTS
VITE_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Настройки умного контекста
VITE_VECTOR_SEARCH_ENABLED=true
VITE_CONTEXT_MAX_RESULTS=10
VITE_CONTEXT_MIN_SCORE=0.6
VITE_CONTEXT_TIME_WINDOW_HOURS=24