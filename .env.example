# OpenRouter API Configuration
VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here

# OpenAI API Configuration (for embeddings)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# ElevenLabs API Configuration (for Text-to-Speech)
VITE_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Qdrant Vector Database Configuration
VITE_QDRANT_URL=http://localhost:6333
VITE_QDRANT_API_KEY=your_qdrant_api_key_here

# Supabase Configuration (if using cloud vector storage)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Development Configuration
VITE_APP_ENV=development
VITE_DEBUG_MODE=true

# Voice Recognition Configuration
VITE_VOICE_LANGUAGE=ru-RU
VITE_VOICE_CONTINUOUS=true

# UI Configuration
VITE_DEFAULT_THEME=dark
VITE_ANIMATION_ENABLED=true